{
  "expo": {
    "name": "Collectify",
    "slug": "capstone-v1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/collectify-icon.png",
    "userInterfaceStyle": "light",
    
    "splash": {
      "image": "./assets/splash-screen.jpeg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true, 
      "splash": {
        "image": "./assets/splash-screen.jpeg",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      },

    
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/collectify-icon.png",
        "backgroundColor": "#ffffff",
        "androidUsesCleartextTraffic": true
      }
    },
    "web": {
      "favicon": "./assets/collectify-icon.png"
    }   
    
  }
}
